#include <iostream>
#include <string>

using namespace std;

int main()
{
    string line;
    bool firstMaze = true;

    while (getline(cin, line))
    {
        if (line.empty())
        {

            cout << '\n';
            continue;
        }

        int count = 0;
        for (char ch : line)
        {
            if (isdigit(ch))
            {
                count += ch - '0';
            }
            else if (ch == '!')
            {
                cout << '\n';
            }
            else
            {
                char toPrint = (ch == 'b') ? ' ' : ch;
                for (int i = 0; i < count; ++i)
                    cout << toPrint;
                count = 0;
            }
        }
        cout << '\n';
    }

    return 0;
}
